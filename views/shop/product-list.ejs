<%- include('../includes/header.ejs') %>   

<!-- NAVBAR -->
<%- include('../includes/nav.ejs'); %>

  <div class="container">
    <% if(prods.length > 0){ %>
    <% for (let product of prods ){ %>
      <div class="row">
        <div class="col-sm-3"></div>
        <div class="col-sm-6">
          <div class="card">
            <div class="card-body">
              <h3>Name : <%= product.title %></h3>
            </div>
          </div>
        </div>
        <div class="col-sm-3"></div>
      </div>
      <% }%>
      <% } else {%>
        <h3>No Products</h3>
      <% }%>
  </div>



